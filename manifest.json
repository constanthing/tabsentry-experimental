{
  "manifest_version": 3,
  "name": "TabSentry (Experimental Dev Build)",
  "version": "0.0.1",
  "description": "Tab Management",
  "permissions": [
    "tabs",
    "tabGroups",
    "windows",
    "bookmarks",
    "alarms",
    "history",
    "favicon"
  ],
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "TabSentry"
  }
}